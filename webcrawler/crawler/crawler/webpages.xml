<?xml version="1.0"?>
<webpages>

    <webpage url="dn.se">
        <article-qualification>
            <xpath>div[@id="article"]</xpath>
            <xpath>div/p[@class="header"]</xpath>
        </article-qualification>

        <article-data>

            <title>
                <xpath>//*[@id="article-content"]/h1/text()</xpath>
            </title>

            <summary>
                <xpath>//*[@id="article-content"]//*[@class="preamble"]//p/text()</xpath>
            </summary>

            <author>
                <xpath>//*[@id="byline"]/div/p/strong/text()</xpath>
            </author>

            <date>
                <xpath>//*[@id="article-content"]/p/text()</xpath>
            </date>

        </article-data>
    </webpage>
    
    
    
    <webpage url="guardian.co.uk">
        <article-qualification>
            <xpath>div[contains(@class,'article')]</xpath>
        </article-qualification>

        <article-data>

            <title>
                <xpath>//*[@id="main-article-info"]/h1/text()</xpath>
            </title>

            <summary>
                <xpath>//*[@id="main-article-info"]//p[@itemprop="description"]/text()</xpath>
            </summary>

            <author>
                <xpath>//*[@id="content"]//a[contains(@class,"contributor")/text()</xpath>
            </author>

            <date>
                <xpath>//*[contains(@class, "publication")]//time/text()</xpath>
            </date>

        </article-data>
    </webpage>
    
    
    
