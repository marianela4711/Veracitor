<?xml version="1.0"?>
<webpages>

    <webpage domain="www.dn.se">
        <article-qualification>
            <xpath>//div[@id="article"]</xpath>
            <xpath>//meta[@content="article"]</xpath>
            <xpath>//div/p[@class="header"]</xpath>
        </article-qualification>

        <article-data>

            <title>
                <xpath>//*[@id="article-content"]//h1/text()</xpath>
                <xpath>//*[@id="main"]//h1/text()</xpath>
                <xpath>//*[contains(@class, "megabauta-articlestart")]//h1/text()</xpath>
                <xpath>//h1[1]/text()</xpath>
            </title>

            <summary>
                <xpath>//*[@id="article-content"]//*[@class="preamble"]//p/text()</xpath>
            </summary>

            <author>
                <xpath>//*[@id="byline"]/div/p/strong/text()</xpath>
            </author>

            <date>
                <xpath>//*[@id="article-content"]/p/text()</xpath>
            </date>

        </article-data>
    </webpage>
    
    
    
    <webpage domain="www.guardian.co.uk">
        <article-qualification>
            <xpath>//div[contains(@class,'article')]</xpath>
        </article-qualification>

        <article-data>

            <title>
                <xpath>//*[@id="main-article-info"]/h1/text()</xpath>
            </title>

            <summary>
                <xpath>//*[@id="main-article-info"]//p[@itemprop="description"]/text()</xpath>
            </summary>

            <author>
                <xpath>//*[@id="content"]//a[contains(@class,"contributor")]/text()</xpath>
            </author>

            <date>
                <xpath>//*[contains(@class, "publication")]//time/text()</xpath>
            </date>

        </article-data>
    </webpage>
    
    
    
     <webpage domain="www.svd.se">
        <article-qualification>
            <xpath>//div[contains(@id,'article-content')]</xpath>
        </article-qualification>

        <article-data>

            <title>
                <xpath>//*[@id="article"]/h1/text()</xpath>
            </title>

            <summary>
                <xpath>//*[@id="article"]/*[contains(@class,"preamble")]/text()</xpath>
            </summary>

            <author>
                <xpath>//*[contains(@class, "author")]/text()</xpath>
            </author>

            <date>
                <xpath>//*[contains(@class, "article-metadata")]//*[contains(@class, "publishdate")]/text()</xpath>
            </date>

        </article-data>
    </webpage>
    
    
    
    <webpage domain="www.nytimes.com">
        <article-qualification>
            <xpath>//div[contains(@id,'article')]</xpath>
        </article-qualification>

        <article-data>

            <title>
                <xpath>//h1[contains(@class,"articleHeadline")]/nyt_headline/text()</xpath>
            </title>

            <summary>
                <xpath>//nyt_text/*[@itemprop="articleBody"][1]/text()</xpath>
            </summary>

            <author>
                <xpath>//*[contains(@itemprop,"author")]//*[contains(@itemprop, "name")]/text()</xpath>
            </author>

            <date>
                <xpath>//*[contains(@class, "dateline")]/text()</xpath>
            </date>

        </article-data>
    </webpage>
    
    
    
    <webpage domain="www.dailymail.co.uk">
        <article-qualification>
            <xpath>//div[contains(@class,'articlePage')]</xpath>
        </article-qualification>

        <article-data>

            <title>
                <xpath>//*[contains(@class, "article-text")]/h1[1]/text()</xpath>
            </title>

            <summary>
                <xpath>//*[contains(@class, "article-text")]//li[1]/font/text()</xpath>
            </summary>

            <author>
                <xpath>//*[contains(@class, "author")]/text()</xpath>
            </author>

            <date>
                <xpath>//*[contains(@class, "article-timestamp")][1]/text()</xpath>
            </date>

        </article-data>
    </webpage>
    
    
    
    <webpage domain="timesofindia.indiatimes.com">
        <article-qualification>
            <xpath>//*[contains(@class,'article')]</xpath>
        </article-qualification>

        <article-data>

            <title>
                <xpath>//*[contains(@class, "arttle")]/h1/text()</xpath>
            </title>

            <summary>
                <xpath>//*[contains(@class, "article-text")]//li[1]/font/text()</xpath>
            </summary>

            <author>
                <xpath>//*[contains(@class, "author")]/text()</xpath>
            </author>

            <date>
                <xpath>//*[contains(@class, "article-timestamp")][1]/text()</xpath>
            </date>

        </article-data>
    </webpage>
    
    
    
    


</webpages>

